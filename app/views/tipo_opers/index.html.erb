<h1>Listing tipo_opers</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_opers.each do |tipo_oper| %>
      <tr>
        <td><%= tipo_oper.nombre %></td>
        <td><%= link_to 'Show', tipo_oper %></td>
        <td><%= link_to 'Edit', edit_tipo_oper_path(tipo_oper) %></td>
        <td><%= link_to 'Destroy', tipo_oper, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo oper', new_tipo_oper_path %>
